{
  "name": "analytics-service",
  "version": "1.0.0",
  "description": "Analytics service for logistics shipping and route management",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "init:all": "ts-node scripts/init-all.ts",
    "init:db": "ts-node scripts/init-db.ts",
    "init:redis": "ts-node scripts/init-redis.ts",
    "init:rabbitmq": "ts-node scripts/init-rabbitmq.ts",
    "init:sonarqube": "ts-node scripts/init-sonarqube.ts"
  },
  "dependencies": {
    "amqplib": "^0.10.9",
    "axios": "^1.7.9",
    "express": "^5.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.14.3",
    "swagger-ui-express": "^5.0.0",
    "zod": "^4.1.4"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.4",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^22.10.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "jest": "^30.1.1",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
} 